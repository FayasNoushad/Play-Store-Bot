{
    "name": "Play-Store-Bot",
    "description": "A play store search telegram bot",
    "repository": "https://github.com/FayasNoushad/Play-Store-Bot",
    "keywords": [
        "search",
        "playstore",
        "apps_search"
    ],
    "env": {
        "BOT_TOKEN": {
            "description": "Your Bot token from @Botfather"
        },
        "API_ID": {
            "description": "Your API_ID from https://my.telegram.org/apps"
        },
        "API_HASH": {
            "description": "Your API_HASH from https://my.telegram.org/apps"
        }
    },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
